getInfo(array team,array tempOption){
  loop 1[x] <-- loops for the length of team
    arrayOfTeamInfo[x]=team[x]
  loop 1
  loop 2[x] <-- loops for the number of options there is (number of teams, number of parks, number of times,etc)
    arrayOfoption[x]=tempOption[x] 
      //option <-- number of options for preferences/choices (ex: park, team, time), tempOtion <-- temporary hold it
  loop 2

}
